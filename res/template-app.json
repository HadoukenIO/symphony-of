{
    "devtools_port": 9090,
    "offlineAccess": true,
    "splashScreenImage": "",
    "startup_app": {
        "autoShow": true,
        "frame": true,
        "minHeight": 625,
        "defaultHeight": 625,
        "defaultWidth": 800,
        "minWidth": 625,
        "saveWindowState": false,
        "spellCheck": true,
        "experimental": {
            "api": {
                "fileDownloadApi": true
            }
        }
    },
    "runtime": {
        "version": "beta",
        "fallbackVersion": "8.56.30.57",
        "arguments": "--noerrdialogs --frameStrategy=frames --enable-aggressive-domstorage-flushing --winhttp-proxy-resolver --enable-chromium-renderer-fork "
    },
    "shortcut": {
        "target": ["start-menu", "desktop", "automatic-start-up"]
    },
    "appAssets": [
        {
            "version": "5.0.0",
            "alias": "ScreenSnippet",
            "target": "OF-ScreenSnippet/ScreenSnippet.exe"
        }
    ]
}